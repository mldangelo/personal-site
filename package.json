    "react-markdown": "^4.0.3",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "snyk": "^1.108.2",
    "stats-webpack-plugin": "^0.7.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.25.1",
    "webpack-dev-middleware": "^3.4.0",
    "webpack-hot-middleware": "^2.24.3"
  },
  "devDependencies": {
    "@babel/node": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "chai": "^4.2.0",
