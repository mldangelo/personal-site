    "@fortawesome/react-fontawesome": "0.2.0",
    "dayjs": "^1.11.9",
    "gh-pages": "^5.0.0",
    "markdown-to-jsx": "^7.2.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-burger-menu": "^3.0.9",
    "react-dom": "^18.2.0",
    "react-ga": "^3.3.1",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.15.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "react-snap": "^1.23.0",
