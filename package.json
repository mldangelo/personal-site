{
  "name": "mldangelo-site",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "webpack -p --config ./webpack/webpack.production.config.js --progress --profile --colors && rimraf ./dist && mv ./tmp ./dist",
    "build-dev": "rimraf ./dist && webpack --config ./webpack/webpack.config.js --progress --profile --colors",
    "clean": "rimraf ./dist && rimraf ./tmp",
    "forever-start": "forever start --minUptime 1000ms --spinSleepTime 1000ms -a -l forever.log -o out.log -e err.log -c 'nodemon server.js --exitcrash --exec babel-node' server.js",
    "forever-stop": "forever stop server.js",
    "dev": "nodemon server.js --ignore app/ --exec babel-node",
    "lint": "./node_modules/.bin/eslint app routes server.js webpack || true",
    "start": "./node_modules/.bin/babel-node server",
    "deploy": "yarn && npm run build && npm run forever-stop || true && npm run forever-start"
  },
  "dependencies": {
    "autoprefixer": "^6.7.2",
    "axios": "^0.15.3",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "body-parser": "^1.16.1",
    "compression": "^1.6.2",
    "dotenv": "^4.0.0",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "forever": "^0.15.3",
    "github": "8.2.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.28.0",
    "markdown-loader": "^2.0.0",
    "moment": "^2.17.1",
    "morgan": "^1.8.1",
    "normalize.css": "^5.0.0",
    "react": "^15.4.2",
    "react-burger-menu": "^1.10.12",
    "react-dom": "^15.4.2",
    "react-ga": "^2.1.2",
    "react-markdown": "^2.4.4",
    "react-router": "^3.0.2",
    "rimraf": "^2.5.4",
    "stats-webpack-plugin": "^0.4.3",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.0",
    "webpack-hot-middleware": "^2.16.1"
  },
  "devDependencies": {
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "^0.10.0",
    "node-sass": "^4.5.0",
    "nodemon": "^1.11.0",
    "sass-loader": "^6.0.0",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7"
  }
}
